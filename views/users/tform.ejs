<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <nav class="navbar">
      <div class="title-head">
        <span class="image"
          ><img
            src="data:image/jpeg;base64,"
        /></span>
      </div>
    </nav>
    <div class="conatiner">
      <form
        action="/userdashboard/transcriptform"
        method="post"
        novalidate
        class="form-validation"
      >
        <div class="mb-3">
          <label class="form-label" for="year">Year</label>
          <div class="input-group">
            <select class="custom-select" id="inputGroupSelect04" name="transcript[year]">
              <option selected name="">Select Year</option>
              <option name="transcript[year] value="1">First</option>
              <option name="transcript[year] value="2">Second</option>
              <option name="transcript[year] value="3">Third</option>
              <option name="transcript[year] value="4">Fourth</option>
            </select>
          </div>
        </div>

        <div class="mb-3">
          <label class="form-label" for="year">Reason</label>
          <div id="checkbox">
            <div class="form-check">
              <input
                class="form-check-input"
                type="checkbox"
                value="Masters"
                id="1"
                name="transcript[type]"
                onclick="selectOnlyThis(this.id)"
              />
              <label class="form-check-label"> Masters </label>
            </div>
            <div class="form-check">
              <input
                class="form-check-input"
                type="checkbox"
                value="Placements"
                id="2"
                name="transcript[type]"
                onclick="selectOnlyThis(this.id)"
              />
              <label class="form-check-label" for="flexCheckChecked">
                Placements
              </label>
            </div>
            <div class="form-check">
              <input
                class="form-check-input"
                type="checkbox"
                value="Other"
                id="3"
                name="transcript[type]"
                onclick="selectOnlyThis(this.id)"
                checked
              />
              <label class="form-check-label" for="flexCheckChecked">
                Other
              </label>
            </div>
          </div>
        </div>

        <div class="mb-3">
          <label class="form-label" for="description">Description</label>
          <textarea
            class="form-control"
            type="text"
            name="transcript[description]"
            id="description"
            required
          ></textarea>
          <div class="valid-feedback">Looks Good!!</div>
        </div>

        <div class="mb-3">
          <button id="btn-submit" class="btn btn-success" type="submit">
            Send
          </button>
        </div>
      </form>
    </div>
  </body>
  <script>
    (function () {
      "use strict";

      // Fetch all the forms we want to apply custom Bootstrap validation styles to
      const forms = document.querySelectorAll(".form-validation");
      console.log("Hello");

      // Loop over them and prevent submission
      Array.from(forms).forEach(function (form) {
        form.addEventListener(
          "submit",
          function (event) {
            if (!form.checkValidity()) {
              event.preventDefault();
              event.stopPropagation();
            }

            form.classList.add("was-validated");
          },
          false
        );
      });
    })();
  </script>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
    crossorigin="anonymous"
  ></script>
  <script src="/javascript/check.js"></script>
</html>
